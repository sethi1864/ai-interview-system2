{
  "name": "ai-interview-system",
  "version": "1.0.0",
  "description": "AI-powered interview system with photorealistic avatars",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "test": "jest",
    "lint": "eslint .",
    "docker-build": "docker build -t ai-interview-system .",
    "docker-run": "docker run -p 3000:3000 ai-interview-system"
  },
  "keywords": [
    "ai",
    "interview",
    "recruitment",
    "avatar",
    "voice-synthesis",
    "real-time"
  ],
  "author": "AI Interview System Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "mongoose": "^7.5.0",
    "redis": "^4.6.8",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.3.1",
    "openai": "^4.0.0",
    "axios": "^1.5.0",
    "multer": "^1.4.5-lts.1",
    "aws-sdk": "^2.1438.0",
    "uuid": "^9.0.0",
    "moment": "^2.29.4",
    "winston": "^3.10.0",
    "compression": "^1.7.4",
    "express-validator": "^7.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "jest": "^29.6.2",
    "eslint": "^8.47.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}